{
	"productName": "quak",
	"version": "0.1.0",
	"identifier": "quak.dev",
	"build": {
		"frontendDist": "./dist",
		"devUrl": "http://localhost:5173/tauri/index.html",
		"beforeDevCommand": "npx vite",
		"beforeBuildCommand": "deno task build --tauri"
	},
	"app": {
		"windows": [
			{
				"title": "quak",
				"width": 800,
				"height": 600,
				"resizable": true,
				"fullscreen": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
